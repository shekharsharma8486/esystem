      <script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {

        });

        function callAjax() {
           
            if ($('#txtOldPwd').val().trim() == "") {
                $('#lblid').show();

                $('#lblid').text("Please Enter Old Password");
                $('#lblid').fadeToggle(3500);

                return false;

            }
            if ($('#txtnewPwd').val().trim() == "") {
                $('#lblid').show();

                $('#lblid').text("Please Enter New Password");
                $('#lblid').fadeToggle(3500);

                return false;

            }
            else if ($('#txtConfirmPwd').val().trim() == "") {
                $('#lblid').show();
                $('#lblid').text("Please Enter Confirm Password");
                $('#lblid').fadeToggle(3500);

                return false;

            }
            else if ($('#txtnewPwd').val().trim() != '') {



                $.ajax({
                    type: "POST",
                    url: "Home.aspx/changePassword", // this for calling the web method function in cs code.  
                    data: '{password: "' + $("#<%=txtnewPwd.ClientID%>")[0].value + '" }', // user name or email value  
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: OnSuccess,
                    failure: function (response) {
                        alert(response);
                    }
                });



                // function OnSuccess  
                function OnSuccess(response) {
                    $('#lblid').show();
                    var msg = $("#<%=lblid.ClientID%>")[0];
                    switch (response.d) {
                        case "true":


                            msg.innerHTML = "Password Changed Successfully. ";
                            $('#Div1').fadeToggle(3500);
                            // $('.reminderCont').fadeToggle(3600);

                            //$('.reminderCont').toggleClass("show-daily-notification-active");




                            break;
                        case "false":

                            //                msg.style.display = "block";
                            //                msg.style.color = "green";
                            //                msg.innerHTML = "Email Available";


                            break;
                    }
                }
            }
    return false;
}

    </script>
    
    <script type="text/javascript">
        function clrtext() {

            $('#txtOldPwd').val("");
            $('#txtnewPwd').val("");
            $('#txtConfirmPwd').val("");
            $('#lblid').val("");

            $('.reminderCont').toggleClass("show-daily-notification-active");
            return false;

        }

    </script>
      <script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
    <script type="text/javascript">

        function onchge() {

            if ($('#txtConfirmPwd').val().trim() != "" && $('#txtnewPwd').val().trim() != "") {
                if ($('#txtConfirmPwd').val().trim() != $('#txtnewPwd').val().trim()) {
                    $('#lblid').show();
                    $('#lblid').text("Password and Confirmed Password Mis-Match.");
                    $('#txtConfirmPwd').val('');
                    $('#txtConfirmPwd').focus();
                    document.getElementById("<%=btnSubmit.ClientID %>").disabled = true;

                }
                if ($('#txtConfirmPwd').val().trim() == $('#txtnewPwd').val().trim()) {
                    document.getElementById("<%=btnSubmit.ClientID %>").disabled = false;
                    $('#lblid').hide();
                }

            }
        }

    </script>

