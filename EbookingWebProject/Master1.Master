<script type="text/javascript">

    function PasswordAvailability() { //This function call on text change.

        $.ajax({
            type: "POST",
            url: "Home.aspx/checkPassword", // this for calling the web method function in cs code.  
            data: '{userpassword: "' + $("#<%=txtOldPwd.ClientID%>")[0].value + '" }', // user name or email value  
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: OnSuccess,
            failure: function (response) {
                alert(response);
            }
        });
    }

    // function OnSuccess  
    function OnSuccess(response) {
        var msg = $("#<%=lblid.ClientID%>")[0];
        switch (response.d) {
            case "true":

                document.getElementById("<%=btnSubmit.ClientID %>").disabled = false;
                msg.innerHTML = "";
                break;
            case "false":
                msg.style.display = "block";
                msg.style.color = "red";
                msg.innerHTML = "Old Password Mis-Match.";
                document.getElementById("<%=txtOldPwd.ClientID %>").focus();
                document.getElementById("<%=btnSubmit.ClientID %>").disabled = true;

                //                msg.style.display = "block";
                //                msg.style.color = "green";
                //                msg.innerHTML = "Email Available";


                break;
        }
    }

    </script>
