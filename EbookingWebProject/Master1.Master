            $('.show-daily-notificationPwd').click(function () {
                $('.reminderCont').toggleClass("show-daily-notification-active");
                $('#txtOldPwd').val("");
                $('#txtnewPwd').val("");
                $('#txtConfirmPwd').val("");
                $('#lblid').val("");
                $('#lblid').text("");
                $('#Div1').show();

            });

        });


    </script>
